// Inheritance
kelas Binatang mewarisi Objek {
    // variable `nama` and `spesies` are both of type `frasa`
    nama jenis frasa;
    spesies jenis frasa;

    // ANOTHER OPTION
    -> can be `frasa` or `ayat`
    nama: ayat;
    spesies: ayat;

    pembina(nama jenis frasa, spesies jenis frasa) {
        ini.nama = nama;
        ini.spesies = spesies;
    }
}

definisi utama() {
    anjing = cipta Binatang("anjing", "mamalia");
}